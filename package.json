{
  "name": "resize-it",
  "version": "1.0.0",
  "description": "An image processing app built with node.js, TypeScript & sharp",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "jasmine",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "dev": "nodemon src/index.ts",
    "start": "node build/index.js",
    "prebuild": "npm run lint && npm run test",
    "format": "prettier \"src/**/*.ts\" --write "
  },
  "keywords": [
    "Udacity",
    "Image",
    "Processing",
    "API",
    "Node",
    "TypeScript",
    "Sharp",
    "Jasmine",
    "Unit",
    "Testing",
    "ESLint",
    "Prettier"
  ],
  "author": "Ahmed Ismail Abu Qahf",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^6.2.4"
  }
}
