{
  "name": "resize-it",
  "version": "1.0.0",
  "description": "An image processing app built with node.js, TypeScript & sharp",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "jasmine",
    "lint": "eslint .",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "lint:fix": "eslint . --fix",
    "dev": "nodemon src/index.ts",
    "start": "node build/index.js",
    "postbuild": "shx cp -r src/views build",
    "format": "prettier \"src/**/*.ts\" --write "
  },
  "keywords": [
    "Udacity",
    "Image",
    "Processing",
    "API",
    "Node",
    "TypeScript",
    "Sharp",
    "Jasmine",
    "Unit",
    "Testing",
    "ESLint",
    "Prettier"
  ],
  "author": "Ahmed Ismail Abu Qahf",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/hbs": "^4.0.1",
    "@types/helmet": "^4.0.0",
    "@types/jasmine": "^4.3.0",
    "@types/lodash": "^4.14.185",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.16",
    "@types/serve-favicon": "^2.5.3",
    "@types/sharp": "^0.30.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/express-validator": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "hbs": "^4.2.0",
    "helmet": "^6.0.0",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.31.0",
    "supertest": "^6.2.4"
  }
}
